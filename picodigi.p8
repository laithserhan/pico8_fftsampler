version 17
__lua__
--digitized sound playback
--by @musurca

--[[
--pico-8 synth pitch range
c2=0
c3=12
c4=24
c5=36
c6=48
c7=60
eb7=63
--]]

snd={}

function make_note(note, instr, vol, effect)
  return { note + 64*(instr%4) , 16*effect + 2*vol + flr(instr/4) } -- flr may be redundant when this is poke'd into memory
end

function set_note(s, t, note)
  local addr = 0x3200 + 68*s + 2*t
  poke(addr, note[1])
  poke(addr+1, note[2])
end

function set_speed(s, speed)
  poke(0x3200 + 68*s + 65, speed)
end

f_off=0
basechn=0
spd=3

function inc_sample()
 f_off+=1
 if f_off>31 then
 	f_off=f_off-32
 	basechn+=4
	end
end

for i=0,63 do
 set_speed(i,spd)
end

for i=1,#snd do
	sl=snd[i]
	local c=0
	for k=1,#sl,2 do
		n=make_note(sl[k],0,sl[k+1],0)
		set_note(basechn+c,f_off,n)
		c+=1
	end
	inc_sample()
end

function _init()
 cls(1)
 print("digitized sound\nby @musurca")
 print("hit üÖæÔ∏è to play sound",25,42,7)
end

function _update()
 if(btnp(4) or btnp(5)) then 
  music(0)
 end
end
